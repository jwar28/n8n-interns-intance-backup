      "parameters": {
        "jsCode": "const workflows = $input.first().json.workflows || [];\n\nconst basePath = String($input.first().json.basePath ?? '').replace(/^\\/+|\\/+$/g, '');\nconst path = String($input.first().json.path ?? $input.first().json.name ?? '');\n\n// We only consider deletions for files inside basePath (if basePath is set)\nif (basePath) {\n  if (!path.startsWith(basePath + '/')) {\n    return [{ json: { isDeleted: false } }];\n  }\n}\n\n// Extract id from filename: <id>.json (last path segment)\nconst filename = path.split('/').pop();\nconst id = filename?.endsWith('.json') ? filename.slice(0, -5) : filename;\n\nconst exists = workflows.some(w => String(w.id) === String(id));\nreturn [{ json: { isDeleted: !exists } }];"
      },
