    {
      "parameters": {
        "jsCode": "const orderJsonKeys = (jsonObj) => {\n  const ordered = {};\n  Object.keys(jsonObj).sort().forEach(key => {\n    ordered[key] = jsonObj[key];\n  });\n  return ordered;\n};\n\n// Github item is index 0, n8n item is index 1 (because Switch(github) joins Get file data + Merge index1)\n// When Get file data fails, index 0 can contain {error: ...} or missing content/data.\n\nconst gh = $input.all()[0].json;\nconst n8n = $input.all()[1].json;\n\n// Normalize comparisons: we compare ordered top-level keys (same as you had)\nconst orderedActual = orderJsonKeys(n8n);\n\n// Check if file returned with content\nif (Object.keys(gh).includes('content')) {\n  const origWorkflow = JSON.parse(Buffer.from(gh.content, 'base64').toString());\n  const orderedOriginal = orderJsonKeys(origWorkflow);\n\n  if (JSON.stringify(orderedOriginal) === JSON.stringify(orderedActual)) {\n    gh.github_status = 'same';\n  } else {\n    gh.github_status = 'different';\n    gh.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n  }\n  gh.content_decoded = orderedOriginal;\n\n// File too large path: data contains the raw JSON string\n} else if (Object.keys(gh).includes('data')) {\n  const origWorkflow = JSON.parse(gh.data);\n  const orderedOriginal = orderJsonKeys(origWorkflow);\n\n  if (JSON.stringify(orderedOriginal) === JSON.stringify(orderedActual)) {\n    gh.github_status = 'same';\n  } else {\n    gh.github_status = 'different';\n    gh.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n  }\n  gh.content_decoded = orderedOriginal;\n\n// Not found or new\n} else {\n  gh.github_status = 'new';\n  gh.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n}\n\nreturn $input.all();"
      },
